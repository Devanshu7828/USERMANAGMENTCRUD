<div class="container ">
    <h1 class="fs-1 mt-5">Update Form</h1>
  
    <form action="/update/<%= userData._id%>" method="POST" class="row g-3">
      <div class="col-md-6">
        <label for="inputEmail4" class="form-label">Name</label>
        <input type="text" class="form-control" id="inputEmail4" name="name" value="<%=userData.name%>"/>
      </div>
      <div class="col-md-6">
        <label for="inputEmail4" class="form-label">Email</label>
        <input type="email" class="form-control" id="inputEmail4" name="email" value="<%=userData.email%>" />
      </div>
      <div class="col-md-6">
        <label for="inputPassword4" class="form-label">Password</label>
        <input
          type="password"
          class="form-control"
          id="inputPassword4"
          name="password"
          value="<%=userData.password%>"
        />
      </div>
      <div class="col-12">
        <label for="inputAddress" class="form-label">Address</label>
        <input
          type="text"
          class="form-control"
          id="inputAddress"
          placeholder="1234 Main St"
          name="address"
          value="<%=userData.address%>"
        />
      </div>
  
      <div class="col-md-6">
        <label for="inputCity" class="form-label">City</label>
        <input type="text" class="form-control" id="inputCity" name="city" value="<%=userData.city%>" />
      </div>
  
      <div class="col-md-2">
        <label for="inputZip" class="form-label">Zip</label>
        <input type="text" class="form-control" id="inputZip" name="zip" value="<%=userData.zip%>"/>
      </div>
  
      <div class="col-12">
        <button type="submit" class="btn btn-primary">UPDATE DATA</button>
      </div>
    </form>
  </div>
</div>